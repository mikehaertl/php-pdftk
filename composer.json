{
    "name": "mikehaertl/php-pdftk",
    "description": "A PDF conversion and form utility based on pdftk.",
    "keywords": ["pdf", "pdftk"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Haertl",
            "email": "haertl.mike@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "mikehaertl/php-shellcommand": "^1.7",
        "mikehaertl/php-tmpfile": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "mikehaertl\\pdftk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests"
        }
    }
}
