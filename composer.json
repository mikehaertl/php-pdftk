{
    "name": "mikehaertl/php-pdftk",
    "description": "A PDF conversion and form utility based on pdftk.",
    "keywords": ["pdf", "pdftk"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Haertl",
            "email": "haertl.mike@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "mikehaertl/php-shellcommand": "^1.0.2",
        "mikehaertl/php-tmpfile": "^1.0.0"
    },
    "autoload": {
        "psr-4": {
            "mikehaertl\\pdftk\\": "src/"
        }
    },
    "suggest": {
        "mikehaertl/pdftk2-precise-amd64": "pdftk 2 binary for Ubuntu 12.04 LTS (amd64)"
    }
}
